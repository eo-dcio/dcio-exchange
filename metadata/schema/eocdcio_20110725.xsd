	<!-- CorrelativeProductSpatialExtent -->
	<xs:element name="CorrelativeProductSpatialExtent" type="eocdcio:CorrelativeProductSpatialExtentType" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Defines the spatial extent of the data.  The location on the earh surface may either be indicated by the name of a fixed location, by a point location or by a polygonal footprint.   Both the pointlocation and the footprint are to be expressed in geographical coordinates (WGS84 datum) (CRS:84).  The altitude of the data product is indicated by a lowestaltitude and a highestaltitude element that should contain the height above the earth surface expressed in meters.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CorrelativeProductSpatialExtentType" mixed="false">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element ref="eocdcio:fixedLocationName" minOccurs="0"/>
					<xs:sequence>
						<xs:element ref="eocdcio:pointLocation" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Coordinates of a Point Location (GML-Point  WGS84 Geographical Coordinates - CRS:84).  Expected structure is gml:Point/gml:pos.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="gml:multiExtentOf" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Coordinates of the Footpint (GML- MultiPolygon WGS84 Geographical Coordinates - CRS:84). Expected structure is gml:Polygon/gml:exterior/gml:LinearRing/gml:posList.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:element name="lowestAltitude" minOccurs="0"/>
					<xs:element name="highestAltitude" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
