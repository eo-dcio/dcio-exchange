	<!-- CorrelativeProductEquipment -->
	<xs:element name="CorrelativeProductEquipment" type="eocdcio:CorrelativeProductEquipmentType" substitutionGroup="gml:AbstractFeature"/>
	<xs:complexType name="CorrelativeProductEquipmentType" mixed="false">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element ref="eocdcio:platform" minOccurs="0"/>
					<xs:element ref="eocdcio:instrumentModelType" minOccurs="0"/>
					<xs:element ref="eocdcio:instrumentModelOrganisation" minOccurs="0"/>
					<xs:element ref="eocdcio:instrumentModelIdentifier" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CorrelativeProductEquipmentPropertyType">
		<xs:sequence>
			<xs:element ref="eocdcio:CorrelativeProductEquipment"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="instrumentModelType" type="xs:string">
		<xs:annotation>
			<xs:documentation>the type of instrument or numeric model that created the data (GEOMS guidelines)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="instrumentModelOrganisation" type="eocdcio:affiliationCodeType">
		<xs:annotation>
			<xs:documentation>the organisation that owns the instrument/model   (GEOMS guidelines)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="instrumentModelIdentifier" type="xs:integer">
		<xs:annotation>
			<xs:documentation>a unique numeric identifier of the instrument within the organisation (GEOMS guidelines))</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!-- CorrelativeProductResult-->
	<xs:element name="CorrelativeProductResult" type="eocdcio:CorrelativeProductResultType" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Defines the correlative product "result"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CorrelativeProductResultType" mixed="false">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="product" type="eocdcio:ProductInformationArrayType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CorrelativeProductResultPropertyType">
		<xs:sequence>
			<xs:element ref="eocdcio:CorrelativeProductResult"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProductInformationArrayType">
		<xs:sequence>
			<xs:element ref="eocdcio:ProductInformation" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ProductInformation" type="eocdcio:ProductInformationType"/>
	<xs:complexType name="ProductInformationType">
		<xs:sequence>
			<xs:element name="onlineResourceURL" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>URL to a location on an FTP or HTTP site or (OGC) Web Service from which one can obtain the data product itself</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileAccessConstraints" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Constraints in using the files (may correspond to the FILE_ACCESS element of the GEOMS guidelines)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataFileHash" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Base 128 hash calculated on the data file to allow verification of uniqueness and integrity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileFormat" minOccurs="0">
				<xs:annotation>
					<xs:documentation>file format that the product is delivered in</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="HDF4"/>
						<xs:enumeration value="HDF5"/>
						<xs:enumeration value="netCDF3"/>
						<xs:enumeration value="netCDF4"/>
						<xs:enumeration value="AMES"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="cloudCoverPercentage" type="gml:MeasureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Areal percentage of product that is covered with clouds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="snowCoverPercentage" type="gml:MeasureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Areal percentage of product that is covered with snow.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
